{
  "parser": "@typescript-eslint/parser",
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": ["@typescript-eslint"],
  "extends": ["eslint:recommended", "prettier"],
  "rules": {
    "no-undef": 0,
    "no-console": 0,
    "eqeqeq": "error",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["error"]
  }
}
