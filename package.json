{
  "name": "@secured-finance/smart-contracts",
  "version": "0.0.1-beta.8",
  "description": "Secured Finance protocol smart contracts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "hardhat test",
    "test:cov": "hardhat coverage",
    "compile": "hardhat compile",
    "fork": "hardhat fork --network",
    "unfork": "hardhat unfork --forkid",
    "compile:force": "hardhat compile --force",
    "deploy": "hardhat deploy --report-gas --no-compile --network",
    "deploy:force": "hardhat deploy --report-gas --no-compile --reset --network",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier:check": "prettier --check '**/*.{ts,js,json,sol}'",
    "prettier:fix": "prettier --write '**/*.{ts,js,json,sol}'",
    "solhint:check": "solhint 'contracts/**/*.sol'",
    "prepare": "husky install",
    "ganache": "ganache"
  },
  "author": "Secured Finance",
  "license": "ISC",
  "homepage": "https://github.com/secured-finance/smart-contracts#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/secured-finance/smart-contracts"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.4.0",
    "@openzeppelin/contracts": "^4.6.0",
    "abi-to-sol": "^0.2.1",
    "bignumber.js": "^9.0.1",
    "bn.js": "^5.2.0",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-ganache": "^2.0.1",
    "@nomiclabs/hardhat-truffle5": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/test-environment": "^0.1.5",
    "@openzeppelin/test-helpers": "^0.5.15",
    "axios": "^0.27.2",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "dotenv": "^8.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.4.1",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.6.1",
    "ganache": "^7.3.2",
    "ganache-time-traveler": "^1.0.16",
    "hardhat": "^2.7.0",
    "hardhat-contract-sizer": "^2.1.1",
    "hardhat-deploy": "^0.11.2",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.4",
    "prettier": "^2.5.0",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.7.17",
    "typechain": "^4.0.3",
    "typescript": "4.0.5",
    "web3": "^1.6.1",
    "web3-utils": "^1.6.1"
  }
}
