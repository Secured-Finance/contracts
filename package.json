{
  "name": "@secured-finance/smart-contracts",
  "version": "0.1.6-beta.0",
  "description": "Secured Finance protocol smart contracts",
  "scripts": {
    "test": "hardhat test",
    "test:unit": "hardhat test ./test/unit/*.test.ts ./test/unit/**/*.test.ts",
    "test:integration": "hardhat test ./test/integration/*.test.ts",
    "test:performance": "hardhat test ./test/performance/*.test.ts",
    "test:cov": "hardhat coverage --testfiles './test/{unit,integration}/**/*.test.ts'",
    "compile": "hardhat compile",
    "compile:force": "hardhat compile --force",
    "fork": "hardhat fork --network",
    "unfork": "hardhat unfork --forkid",
    "deploy": "hardhat deploy --report-gas --no-compile --network",
    "deploy:force": "hardhat deploy --report-gas --no-compile --reset --network",
    "lint:check": "eslint . --ext js,ts",
    "lint:fix": "eslint --fix . --ext js,ts",
    "prettier:check": "prettier --check '**/*.{ts,js,json,sol}'",
    "prettier:fix": "prettier --write '**/*.{ts,js,json,sol}'",
    "solhint:check": "solhint 'contracts/**/*.sol'",
    "prepare": "husky install",
    "ganache": "ganache",
    "prepublishOnly": "npm run compile",
    "docgen": "hardhat docgen",
    "predocgen": "rm -rf cache && npm run compile:force && rm -rf docs",
    "flatten": "./scripts/hardhat-flattener.sh",
    "security:slither": "slither flattened --solc-disable-warnings --exclude-informational --print human-summary 2>&1 | cat > secured-finance-slither.txt",
    "security:mythril": "docker run --entrypoint /bin/bash -v $(pwd):/home/mythril mythril/myth /home/mythril/scripts/mythril.sh"
  },
  "author": "Secured Finance",
  "license": "ISC",
  "homepage": "https://github.com/secured-finance/smart-contracts#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/secured-finance/smart-contracts"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.4.0",
    "@openzeppelin/contracts": "^4.7.3",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-ganache": "^2.0.1",
    "@nomiclabs/hardhat-truffle5": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/test-environment": "^0.1.5",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.14",
    "axios": "^0.27.2",
    "babel-eslint": "^10.1.0",
    "bignumber.js": "^9.1.0",
    "chai": "^4.3.6",
    "dotenv": "^8.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.4.1",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.6.1",
    "ganache": "^7.3.2",
    "ganache-time-traveler": "^1.0.16",
    "hardhat": "^2.7.0",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-deploy": "^0.11.2",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.4",
    "moment": "^2.29.4",
    "prettier": "^2.5.0",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.7.17",
    "solidity-docgen": "^0.6.0-beta.25",
    "ts-node": "^10.9.1",
    "typechain": "^4.0.3",
    "typescript": "^4.0.5",
    "web3": "^1.6.1"
  }
}
